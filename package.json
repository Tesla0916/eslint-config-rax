{
  "name": "@teslao/eslint-config-rax",
  "version": "0.0.1",
  "description": "A universal tslint config.",
  "license": "BSD-3-Clause",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx --ext .ts ./*.js",
    "baseLint": "eslint --ext .js --ext .jsx --ext .ts ./test/base",
    "tsLint": "eslint --ext .js --ext .jsx --ext .ts ./test/typescript",
    "reactLint": "eslint --ext .js --ext .jsx --ext .ts ./test/react",
    "tsReactLint": "eslint --ext .js --ext .jsx --ext .tsx ./test/typescript-react"
  },
  "keywords": [
    "universal",
    "eslint",
    "tslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raxjs/eslint-config-rax"
  },
  "bugs": {
    "url": "https://github.com/raxjs/eslint-config-rax/issues"
  },
  "homepage": "https://github.com/raxjs/eslint-config-rax#readme",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.25.1",
    "typescript": "^4.4.2"
  }
}
